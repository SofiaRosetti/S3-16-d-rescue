       android:insetTop="@dimen/inset"
       android:insetRight="@dimen/inset"
       android:insetBottom="@dimen/inset">

    <shape android:shape="rectangle">

        <corners android:radius="@dimen/radius_small"/>

        <solid android:color="@color/colorTextIcons"/>

        <stroke
                android:color="@color/colorPrimaryDark"
                android:width="@dimen/border_width"/>

    </shape>

</inset>